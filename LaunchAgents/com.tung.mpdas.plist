<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.tung.mpdas</string>
    <key>WorkingDirectory</key>
    <string>~</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/tung/local/bin/mpdas</string>
        <string>-c</string>
        <string>~/.mpdasrc</string>
    </array>
    <key>EnableGlobbing</key>
    <true/>
