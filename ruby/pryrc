
# Pry Config
Pry.config.editor = "vim"
Pry.config.prompt = [proc { |obj, nest_level| "#{RUBY_VERSION} (#{obj}):#{nest_level} > " }, proc { |obj, nest_level| "#{RUBY_VERSION} (#{obj}):#{nest_level} * " }]

## Useful Collections
def a
  (1..6).to_a
end

def h
  {hello: "world", free: "of charge"}
end

def d
  rand(4) + 1
end

